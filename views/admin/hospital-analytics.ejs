<div class="p-6 md:p-8 lg:p-10">

<div class="mb-6">
    <h1 class="text-3xl font-bold text-slate-800">Hospital Analytics</h1>
    <p class="text-slate-600 mt-2">Monitor hospital performance, bed occupancy, and resource utilization</p>
</div>

<!-- Hospital Selection Card -->
<div class="bg-white rounded-2xl shadow-lg border border-slate-200 p-6 mb-8">
    <div class="flex items-center justify-between mb-4">
        <h2 class="text-xl font-semibold text-slate-800">Select Hospital</h2>
        <span id="selectedHospitalBadge" class="hidden px-3 py-1 bg-emerald-100 text-emerald-700 text-sm font-medium rounded-full">
            <svg class="w-4 h-4 inline mr-1" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path>
            </svg>
            <span id="selectedHospitalName"></span>
        </span>
    </div>
    
    <!-- Search Input -->
    <div class="mb-4">
        <div class="relative">
            <input 
                type="text" 
                id="hospitalSearch" 
                placeholder="Search hospital by name..." 
                class="w-full px-4 py-3 pl-11 border border-slate-300 rounded-xl focus:ring-2 focus:ring-emerald-500 focus:border-emerald-500 outline-none transition-all"
            >
            <svg class="w-5 h-5 text-slate-400 absolute left-3 top-3.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z"></path>
            </svg>
        </div>
    </div>

    <!-- Hospital List -->
    <div class="max-h-96 overflow-y-auto custom-scrollbar">
        <div id="hospitalList" class="space-y-2">
            <% if (hospitals && hospitals.length > 0) { %>
                <% hospitals.forEach(hospital => { %>
                    <div class="hospital-item p-4 border border-slate-200 rounded-xl hover:border-emerald-500 hover:bg-emerald-50 cursor-pointer transition-all" 
                         data-hospital-id="<%= hospital._id %>" 
                         data-hospital-name="<%= hospital.hospitalName || 'Unnamed Hospital' %>">
                        <div class="flex items-start justify-between">
                            <div class="flex-1">
                                <h3 class="font-semibold text-slate-800 mb-1"><%= hospital.hospitalName || 'Unnamed Hospital' %></h3>
                                <p class="text-sm text-slate-600 flex items-center">
                                    <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z"></path>
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z"></path>
                                    </svg>
                                    <%= hospital.localArea || hospital.zone || hospital.address || 'Location not specified' %>
                                </p>
                            </div>
                            <div class="ml-4">
                                <span class="px-3 py-1 bg-emerald-100 text-emerald-700 text-xs font-medium rounded-full">Active</span>
                            </div>
                        </div>
                    </div>
                <% }); %>
            <% } else { %>
                <div class="text-center py-8">
                    <svg class="w-16 h-16 mx-auto text-slate-300 mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
                    </svg>
                    <p class="text-slate-500">No hospitals registered yet</p>
                </div>
            <% } %>
        </div>
        
        <!-- No Results Message -->
        <div id="noResults" class="hidden text-center py-8">
            <svg class="w-16 h-16 mx-auto text-slate-300 mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
            </svg>
            <p class="text-slate-500">No hospitals found matching your search</p>
        </div>
    </div>
</div>

<!-- Hospital-Specific Analytics Section -->
<div id="hospitalAnalytics" class="hidden">
    <div class="mb-6">
        <h2 class="text-2xl font-bold text-slate-800">Hospital Performance Metrics</h2>
        <p class="text-slate-600 mt-1">Analytics for <span id="analyticsHospitalName" class="font-semibold text-emerald-600"></span></p>
    </div>

    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 mb-8">
        <!-- Bed Occupancy Card -->
        <div class="bg-white rounded-2xl shadow-sm border border-slate-200 p-6">
            <div class="flex items-center justify-between mb-4">
                <h3 class="text-lg font-semibold text-slate-800">Bed Occupancy</h3>
                <div class="p-3 bg-blue-100 rounded-xl">
                    <svg class="w-6 h-6 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6"></path>
                    </svg>
                </div>
            </div>
            <div class="mb-2">
                <div class="flex items-end justify-between mb-1">
                    <span class="text-3xl font-bold text-slate-800">0</span>
                    <span class="text-sm text-slate-600">/ 0 beds</span>
                </div>
                <div class="w-full bg-slate-200 rounded-full h-2">
                    <div class="bg-blue-600 h-2 rounded-full" style="width: 0%"></div>
                </div>
            </div>
            <p class="text-sm text-slate-600 mt-3">Current occupancy rate: <span class="font-semibold">0%</span></p>
        </div>

        <!-- Resource Status Card -->
        <div class="bg-white rounded-2xl shadow-sm border border-slate-200 p-6">
            <div class="flex items-center justify-between mb-4">
                <h3 class="text-lg font-semibold text-slate-800">Resource Status</h3>
                <div class="p-3 bg-emerald-100 rounded-xl">
                    <svg class="w-6 h-6 text-emerald-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path>
                    </svg>
                </div>
            </div>
            <div class="space-y-3">
                <div class="flex items-center justify-between">
                    <span class="text-sm text-slate-600">Doctors</span>
                    <span class="text-sm font-semibold text-slate-800">0</span>
                </div>
                <div class="flex items-center justify-between">
                    <span class="text-sm text-slate-600">Nurses</span>
                    <span class="text-sm font-semibold text-slate-800">0</span>
                </div>
                <div class="flex items-center justify-between">
                    <span class="text-sm text-slate-600">Equipment</span>
                    <span class="text-sm font-semibold text-slate-800">0</span>
                </div>
            </div>
        </div>

        <!-- Patient Flow Card -->
        <div class="bg-white rounded-2xl shadow-sm border border-slate-200 p-6">
            <div class="flex items-center justify-between mb-4">
                <h3 class="text-lg font-semibold text-slate-800">Patient Flow</h3>
                <div class="p-3 bg-purple-100 rounded-xl">
                    <svg class="w-6 h-6 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"></path>
                    </svg>
                </div>
            </div>
            <div class="space-y-3">
                <div class="flex items-center justify-between">
                    <span class="text-sm text-slate-600">Today's Admissions</span>
                    <span class="text-sm font-semibold text-emerald-600">0</span>
                </div>
                <div class="flex items-center justify-between">
                    <span class="text-sm text-slate-600">Today's Discharges</span>
                    <span class="text-sm font-semibold text-blue-600">0</span>
                </div>
                <div class="flex items-center justify-between">
                    <span class="text-sm text-slate-600">Active Patients</span>
                    <span class="text-sm font-semibold text-slate-800">0</span>
                </div>
            </div>
        </div>
    </div>

    <!-- Charts Section -->
    <div class="grid grid-cols-1 lg:grid-cols-3 gap-6 mb-8">
        <!-- Bed Occupancy Stacked Bar Chart -->
        <div class="lg:col-span-2 bg-white rounded-2xl shadow-sm border border-slate-200 p-6">
            <div class="mb-4">
                <h3 class="text-lg font-semibold text-slate-800">Bed Occupancy Analysis</h3>
                <p class="text-sm text-slate-600 mt-1" id="bedChartSubtitle">Occupied vs Available beds across hospitals</p>
            </div>
            <div class="relative h-80">
                <canvas id="bedOccupancyChart"></canvas>
            </div>
        </div>

        <!-- Right Side Panel -->
        <div class="space-y-6">
            <!-- Medicine Stock Gauge -->
            <div class="bg-white rounded-2xl shadow-sm border border-slate-200 p-6">
                <div class="mb-4">
                    <h3 class="text-lg font-semibold text-slate-800">Medicine Stock</h3>
                    <p class="text-sm text-slate-600 mt-1">Current inventory level</p>
                </div>
                <div class="relative h-48 flex items-center justify-center">
                    <canvas id="medicineGaugeChart"></canvas>
                </div>
                <div class="mt-4 text-center">
                    <p class="text-sm text-slate-600">Stock Level: <span id="stockPercentage" class="font-semibold text-slate-800">--</span></p>
                </div>
            </div>

            <!-- Hospital Health Score -->
            <div class="bg-white rounded-2xl shadow-sm border border-slate-200 p-6">
                <div class="mb-4">
                    <h3 class="text-lg font-semibold text-slate-800">Health Score</h3>
                    <p class="text-sm text-slate-600 mt-1">Overall performance rating</p>
                </div>
                <div class="flex items-center justify-center py-4">
                    <div class="relative">
                        <svg class="transform -rotate-90 w-32 h-32">
                            <circle cx="64" cy="64" r="56" stroke="#e2e8f0" stroke-width="12" fill="none"/>
                            <circle id="healthScoreCircle" cx="64" cy="64" r="56" stroke="#10b981" stroke-width="12" fill="none"
                                stroke-dasharray="351.86" stroke-dashoffset="351.86" stroke-linecap="round"/>
                        </svg>
                        <div class="absolute inset-0 flex items-center justify-center flex-col">
                            <span id="healthScoreValue" class="text-3xl font-bold text-slate-800">--</span>
                            <span class="text-xs text-slate-500 mt-1">out of 100</span>
                        </div>
                    </div>
                </div>
                <div class="space-y-2 mt-4">
                    <div class="flex items-center justify-between text-sm">
                        <span class="text-slate-600">Bed Management</span>
                        <span id="bedManagementScore" class="font-semibold text-slate-800">--</span>
                    </div>
                    <div class="flex items-center justify-between text-sm">
                        <span class="text-slate-600">Resource Availability</span>
                        <span id="resourceScore" class="font-semibold text-slate-800">--</span>
                    </div>
                    <div class="flex items-center justify-between text-sm">
                        <span class="text-slate-600">Patient Care</span>
                        <span id="patientCareScore" class="font-semibold text-slate-800">--</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Empty State - When no hospital is selected -->
<div id="emptyState" class="text-center py-16">
    <svg class="w-24 h-24 mx-auto text-slate-300 mb-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"></path>
    </svg>
    <h3 class="text-xl font-semibold text-slate-800 mb-2">Select a Hospital</h3>
    <p class="text-slate-600">Choose a hospital from the list above to view detailed analytics</p>
</div>

<script>
    // Hospital selection functionality
    const hospitalItems = document.querySelectorAll('.hospital-item');
    const hospitalSearch = document.getElementById('hospitalSearch');
    const hospitalAnalytics = document.getElementById('hospitalAnalytics');
    const emptyState = document.getElementById('emptyState');
    const selectedHospitalBadge = document.getElementById('selectedHospitalBadge');
    const selectedHospitalName = document.getElementById('selectedHospitalName');
    const analyticsHospitalName = document.getElementById('analyticsHospitalName');
    const noResults = document.getElementById('noResults');

    // Chart instances
    let bedOccupancyChart = null;
    let medicineGaugeChart = null;

    // Initialize charts with all hospitals data on page load
    async function initializeChartsWithAllHospitals() {
        const hospitalsData = [];
        
        <% if (hospitals && hospitals.length > 0) { %>
            <% hospitals.forEach(hospital => { %>
                hospitalsData.push({
                    name: '<%= hospital.hospitalName || "Hospital" %>',
                    totalBeds: <%= (hospital.beds?.general?.total || 0) + (hospital.beds?.icu?.total || 0) + (hospital.beds?.isolation?.total || 0) %>,
                    availableBeds: <%= (hospital.beds?.general?.available || 0) + (hospital.beds?.icu?.available || 0) + (hospital.beds?.isolation?.available || 0) %>
                });
            <% }); %>
        <% } %>
        
        // Calculate occupied beds
        hospitalsData.forEach(h => {
            h.occupiedBeds = h.totalBeds - h.availableBeds;
        });
        
        // Initialize bed occupancy chart
        initBedOccupancyChart(hospitalsData);
    }

    // Initialize bed occupancy stacked bar chart
    function initBedOccupancyChart(data) {
        const ctx = document.getElementById('bedOccupancyChart');
        
        if (bedOccupancyChart) {
            bedOccupancyChart.destroy();
        }
        
        bedOccupancyChart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels: data.map(h => h.name),
                datasets: [{
                    label: 'Occupied Beds',
                    data: data.map(h => h.occupiedBeds),
                    backgroundColor: '#ef4444',
                    borderRadius: 6,
                }, {
                    label: 'Available Beds',
                    data: data.map(h => h.availableBeds),
                    backgroundColor: '#10b981',
                    borderRadius: 6,
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    x: {
                        stacked: true,
                        grid: { display: false },
                        ticks: { 
                            font: { size: 11 },
                            maxRotation: 45,
                            minRotation: 45
                        }
                    },
                    y: {
                        stacked: true,
                        beginAtZero: true,
                        grid: { color: '#f1f5f9' },
                        ticks: { font: { size: 11 } }
                    }
                },
                plugins: {
                    legend: {
                        position: 'top',
                        labels: { 
                            usePointStyle: true,
                            padding: 15,
                            font: { size: 12 }
                        }
                    },
                    tooltip: {
                        backgroundColor: 'rgba(0, 0, 0, 0.8)',
                        padding: 12,
                        titleFont: { size: 13 },
                        bodyFont: { size: 12 },
                        callbacks: {
                            afterBody: function(context) {
                                const index = context[0].dataIndex;
                                const total = data[index].totalBeds;
                                const occupied = data[index].occupiedBeds;
                                const percent = total > 0 ? Math.round((occupied / total) * 100) : 0;
                                return `\nTotal: ${total} beds\nOccupancy: ${percent}%`;
                            }
                        }
                    }
                }
            }
        });
    }

    // Initialize medicine gauge chart
    function initMedicineGaugeChart(percentage) {
        const ctx = document.getElementById('medicineGaugeChart');
        
        if (medicineGaugeChart) {
            medicineGaugeChart.destroy();
        }
        
        const getColor = (pct) => {
            if (pct >= 70) return '#10b981'; // Green
            if (pct >= 40) return '#f59e0b'; // Orange
            return '#ef4444'; // Red
        };
        
        medicineGaugeChart = new Chart(ctx, {
            type: 'doughnut',
            data: {
                datasets: [{
                    data: [percentage, 100 - percentage],
                    backgroundColor: [getColor(percentage), '#e2e8f0'],
                    borderWidth: 0,
                    circumference: 180,
                    rotation: 270
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                cutout: '75%',
                plugins: {
                    legend: { display: false },
                    tooltip: { enabled: false }
                }
            },
            plugins: [{
                id: 'gaugeText',
                afterDatasetDraw(chart) {
                    const { ctx, chartArea: { width, height } } = chart;
                    ctx.save();
                    ctx.font = 'bold 24px Inter';
                    ctx.fillStyle = '#1e293b';
                    ctx.textAlign = 'center';
                    ctx.fillText(percentage + '%', width / 2, height / 2 + 20);
                    ctx.restore();
                }
            }]
        });
        
        document.getElementById('stockPercentage').textContent = percentage + '%';
    }

    // Update health score visualization
    function updateHealthScore(score, components) {
        const circumference = 2 * Math.PI * 56; // r = 56
        const offset = circumference - (score / 100) * circumference;
        
        document.getElementById('healthScoreCircle').style.strokeDashoffset = offset;
        document.getElementById('healthScoreValue').textContent = score;
        
        // Update component scores with real data
        if (components) {
            document.getElementById('bedManagementScore').textContent = components.bedManagement;
            document.getElementById('resourceScore').textContent = components.resource;
            document.getElementById('patientCareScore').textContent = components.patientCare;
        } else {
            document.getElementById('bedManagementScore').textContent = '--';
            document.getElementById('resourceScore').textContent = '--';
            document.getElementById('patientCareScore').textContent = '--';
        }
    }

    // Handle hospital selection
    hospitalItems.forEach(item => {
        item.addEventListener('click', async function() {
            // Remove active state from all items
            hospitalItems.forEach(i => {
                i.classList.remove('border-emerald-500', 'bg-emerald-50');
                i.classList.add('border-slate-200');
            });
            
            // Add active state to clicked item
            this.classList.remove('border-slate-200');
            this.classList.add('border-emerald-500', 'bg-emerald-50');
            
            // Get hospital info
            const hospitalId = this.dataset.hospitalId;
            const hospitalName = this.dataset.hospitalName;
            
            console.log('Selected hospital:', { hospitalId, hospitalName });
            
            // Update selected hospital badge
            selectedHospitalName.textContent = hospitalName;
            selectedHospitalBadge.classList.remove('hidden');
            
            // Update analytics section header
            analyticsHospitalName.textContent = hospitalName;
            
            // Show analytics, hide empty state
            hospitalAnalytics.classList.remove('hidden');
            emptyState.classList.add('hidden');
            
            // Fetch hospital data from API
            try {
                const response = await fetch(`/admin/api/hospital/${hospitalId}`);
                
                if (!response.ok) {
                    const errorData = await response.json().catch(() => ({ error: 'Unknown error' }));
                    throw new Error(errorData.error || `HTTP error! status: ${response.status}`);
                }
                
                const data = await response.json();
                
                // Update bed occupancy
                document.querySelector('#hospitalAnalytics .bg-white:nth-child(1) .text-3xl').textContent = data.beds.occupied;
                document.querySelector('#hospitalAnalytics .bg-white:nth-child(1) .text-sm:last-child span').textContent = data.beds.total;
                document.querySelector('#hospitalAnalytics .bg-white:nth-child(1) .bg-blue-600').style.width = `${data.beds.occupancyPercent}%`;
                document.querySelector('#hospitalAnalytics .bg-white:nth-child(1) .text-sm:last-child').innerHTML = 
                    `Current occupancy rate: <span class="font-semibold">${data.beds.occupancyPercent}%</span>`;
                
                // Update resource status
                const resourceCards = document.querySelectorAll('#hospitalAnalytics .bg-white:nth-child(2) .space-y-3 .flex');
                resourceCards[0].querySelector('.font-semibold').textContent = data.resources.doctors;
                resourceCards[1].querySelector('.font-semibold').textContent = data.resources.nurses;
                resourceCards[2].querySelector('.font-semibold').textContent = data.resources.equipment;
                
                // Update patient flow
                const flowCards = document.querySelectorAll('#hospitalAnalytics .bg-white:nth-child(3) .space-y-3 .flex');
                flowCards[0].querySelector('.font-semibold').textContent = data.patientFlow.todaysAdmissions;
                flowCards[1].querySelector('.font-semibold').textContent = data.patientFlow.todaysDischarges;
                flowCards[2].querySelector('.font-semibold').textContent = data.patientFlow.activePatients;
                
                // Update charts for selected hospital
                document.getElementById('bedChartSubtitle').textContent = `Bed occupancy for ${hospitalName}`;
                
                // Update bed chart with single hospital data
                initBedOccupancyChart([{
                    name: hospitalName,
                    occupiedBeds: data.beds.occupied,
                    availableBeds: data.beds.available,
                    totalBeds: data.beds.total
                }]);
                
                // Update medicine gauge
                const medicineStockPercent = data.medicineStock;
                initMedicineGaugeChart(medicineStockPercent);
                
                // Update health score
                const healthScore = data.healthScore;
                updateHealthScore(healthScore, data.healthScoreComponents);
                
            } catch (error) {
                console.error('Error fetching hospital data:', error);
                
                // Hide analytics and show empty state
                hospitalAnalytics.classList.add('hidden');
                emptyState.classList.remove('hidden');
                
                // Remove selection
                this.classList.remove('border-emerald-500', 'bg-emerald-50');
                this.classList.add('border-slate-200');
                selectedHospitalBadge.classList.add('hidden');
                
                // Show error message
                alert(`Error: ${error.message}\n\nThis hospital may have invalid data. Please contact the administrator.`);
                return;
            }
            
            // Scroll to analytics section
            setTimeout(() => {
                hospitalAnalytics.scrollIntoView({ behavior: 'smooth', block: 'start' });
            }, 100);
        });
    });

    // Hospital search functionality
    hospitalSearch.addEventListener('input', function() {
        const searchTerm = this.value.toLowerCase();
        let visibleCount = 0;
        
        hospitalItems.forEach(item => {
            const hospitalName = item.querySelector('h3').textContent.toLowerCase();
            const location = item.querySelector('p').textContent.toLowerCase();
            
            if (hospitalName.includes(searchTerm) || location.includes(searchTerm)) {
                item.style.display = 'block';
                visibleCount++;
            } else {
                item.style.display = 'none';
            }
        });
        
        // Show/hide no results message
        if (visibleCount === 0) {
            noResults.classList.remove('hidden');
        } else {
            noResults.classList.add('hidden');
        }
    });

    // Initialize charts with all hospitals data on page load
    window.addEventListener('DOMContentLoaded', function() {
        initializeChartsWithAllHospitals();
    });
</script>

</div>
